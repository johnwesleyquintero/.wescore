{
  "name": "@wescore/core",
  "version": "1.0.0",
  "description": "A Modern Code Quality Framework for automated checks and reporting",
  "main": "main.js",
  "files": [
    "main.js",
    "config/**/*.js",
    "reporting/**/*.js",
    "runner/**/*.js",
    "utils/**/*.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "build": "node scripts/build.js",
    "cq": "node main.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwesleyquintero/.wescore.git"
  },
  "keywords": [
    "code-quality",
    "linting",
    "formatting",
    "typescript",
    "automation",
    "ci-cd",
    "developer-tools",
    "quality-assurance"
  ],
  "author": "Wescore Contributors",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/johnwesleyquintero/.wescore/issues"
  },
  "homepage": "https://github.com/johnwesleyquintero/.wescore#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
